    "#inverse of the square matrix and matrix solution\n",
    "import numpy as np\n",
    "A=np.array([[4,3],[-5,9]])\n",
    "B=np.array([[20],[26]])\n",
    "Ainv=np.linalg.inv(A)\n",
    "X=Ainv.dot(B)\n",
    "print(X)\n"
   ]
