    "#integration\n",
    "from numpy import exp,pi,cos,sin\n",
    "from scipy.integrate import quad\n",
    "def f(x):\n",
    "    return exp(cos(-2*x*pi)*sin(x))+3.2\n",
    "res,err=quad(f,-2,2)\n",
    "print(\"result of integration is\",res)\n",
    "print(\"error in integration is\",err)"
